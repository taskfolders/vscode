{
  "name": "TaskFolders",
  "displayName": "TaskFolders",
  "publisher": "TaskFolders",
  "description": "my description",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "./assets/logo/128.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/taskfolders/vscode"
  },
  "categories": [
    "Notebooks",
    "Linters",
    "Formatters"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "plaintext",
        "aliases": [
          "Plain Text"
        ],
        "extensions": [
          ".md",
          ".sh",
          ".ts"
        ],
        "filenames": [
          ""
        ],
        "mimeTypes": [
          "text/plain"
        ],
        "scheme": "file"
      }
    ],
    "commands": [
      {
        "command": "code-play.helloWorld",
        "title": "code-play: Hello World"
      },
      {
        "command": "code-play.linkifyUUIDs",
        "title": "code-play: Run linkify"
      },
      {
        "command": "code-play.playHello",
        "title": "code-play: Hello World v2"
      },
      {
        "command": "code-play.openUUID",
        "title": "code-play: Open UUID"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.54.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@vscode/vsce": "^2.22.0",
    "memfs": "^4.6.0",
    "mocha": "^10.2.0",
    "url-regex": "^5.0.0",
    "uuid": "^9.0.1"
  }
}
